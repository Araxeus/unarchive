{
  "name": "unarchive",
  "author": "Araxeus",
  "description": "Automatically extract archives of various types",
  "version": "1.1.4",
  "type": "module",
  "license": "MIT",
  "repository": "https://github.com/Araxeus/unarchive",
  "homepage": "https://github.com/Araxeus/unarchive",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "b": "tsc && bun ignore-tests",
    "check": "biome check",
    "lint": "biome check --write",
    "ci": "bun b && biome ci . && node ./dist/tests/test.js",
    "ignore-tests": "echo '*' > ./dist/tests/.npmignore"
  },
  "dependencies": {
    "compressing": "^2.0.0",
    "file-type": "^21.1.1"
  },
  "keywords": [
    "uncompress",
    "archive",
    "tar",
    "zip",
    "tgz",
    "targz"
  ],
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@types/node": "^24.10.1",
    "typescript": "^5.9.3"
  },
  "packageManager": "bun@1.3.3",
  "trustedDependencies": [
    "@biomejs/biome"
  ]
}
