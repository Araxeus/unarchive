{
  "name": "unarchive",
  "author": "Araxeus",
  "description": "Automatically extract archives of various types",
  "version": "1.2.1",
  "type": "module",
  "license": "MIT",
  "repository": "https://github.com/Araxeus/unarchive",
  "homepage": "https://github.com/Araxeus/unarchive",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "b": "tsc",
    "check": "biome check && tsc --noEmit && bun test",
    "lint": "biome check --write",
    "ci": "biome ci && tsc --noEmit && bun test"
  },
  "dependencies": {
    "compressing": "^2.0.0",
    "file-type": "^21.1.1"
  },
  "keywords": [
    "uncompress",
    "archive",
    "extract",
    "decompress",
    "tar",
    "zip",
    "tgz",
    "targz",
    "xpi",
    "crx",
    "gz",
    "gzip"
  ],
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@types/bun": "^1.3.4",
    "@types/node": "^24.10.1",
    "typescript": "^5.9.3"
  },
  "packageManager": "bun@1.3.4",
  "trustedDependencies": [
    "@biomejs/biome"
  ]
}
