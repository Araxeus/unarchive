{
  "name": "unarchive",
  "author": "Araxeus",
  "description": "Automatically extract archives of various types",
  "version": "1.1.3",
  "type": "module",
  "license": "MIT",
  "repository": "https://github.com/Araxeus/unarchive",
  "homepage": "https://github.com/Araxeus/unarchive",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "b": "tsc && bun ignore-tests",
    "check": "biome check",
    "lint": "biome check --write",
    "ci": "bun b && biome ci . && node ./dist/tests/test.js",
    "ignore-tests": "echo '*' > ./dist/tests/.npmignore",
    "publish": "bun ci && npm publish"
  },
  "dependencies": {
    "compressing": "^1.10.1",
    "file-type": "^19.4.0"
  },
  "keywords": [
    "uncompress",
    "archive",
    "tar",
    "zip",
    "tgz",
    "targz"
  ],
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.7.7",
    "typescript": "^5.6.3"
  },
  "packageManager": "bun@1.1.21",
  "trustedDependencies": [
    "@biomejs/biome"
  ]
}
